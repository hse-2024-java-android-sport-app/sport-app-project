    <fragment
        android:id="@+id/nav_find_friends"
        android:label="@string/menu_find_friends"
        android:name="org.sportApp.userInterface.sportsman.ui.interaction.FindFriends"
        tools:layout="@layout/fragment_find_user" />

    <fragment
        android:id="@+id/nav_rating"
        android:label="@string/menu_rating"
        android:name="org.sportApp.userInterface.sportsman.ui.interaction.Rating"
        tools:layout="@layout/fragment_common" />

    <fragment
        android:id="@+id/nav_notifications"
        android:label="@string/menu_notifications"
        android:name="org.sportApp.userInterface.sportsman.ui.interaction.Notifications"
        tools:layout="@layout/fragment_common" />

